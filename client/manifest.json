{
   "manifest_version": 2,
   "name": "Youtube real time app",
   "version": "0.1.0",
   "description": "Lateral blog post demo extension",
   "icons": {
      "128": "icon128.png",
      "48": "icon48.png"
   },
   "browser_action": {
      "default_icon": {
         "19": "icon19.png",
         "38": "icon38.png"
      },
      "default_title": "DV Youtube"
   },
   "permissions": [
      "tabs"
   ],
   "content_scripts": [
      {
         "css": [
            "styles.css"
         ],
         "js": [
            "node_modules/socket.io-client/dist/socket.io.js",
            "overlay.js",
            "index.js"
         ],
         "matches": [
            "http://*/*",
            "https://*/*"
         ]
      }
   ]
}
